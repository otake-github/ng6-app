{
    "name": "ng6-app",
    "version": "0.0.0",
    "scripts": {
        "ng": "ng",
        "start": "ng serve --proxy-config proxy.conf.json",
        "build": "ng build",
        "test": "ng test",
        "lint": "ng lint",
        "e2e": "ng e2e",
        "tslint": "tslint",
        "start:sv:ts": "ts-node server/server.ts",
        "start:sv": "npm run build:sv & npm run serve:sv",
        "serve:sv": "node dist/server/server.js",
        "serve-debug:sv": "nodemon --inspect dist/server/server.js",
        "watch:sv": "nodemon dist/server/server.js",
        "tsc": "tsc",
        "build:sv": "tsc --project server/tsconfig.server.json",
        "build:sv-w": "tsc --project server/tsconfig.server.json -w",
        "test:sv-build": "tsc --project server/tsconfig.spec.json",
        "test:sv-init:sv": "node ./node_modules/jasmine/bin/jasmine.js init",
        "test:sv-ts": "ts-node -r tsconfig-paths/register --project server/tsconfig.spec.json node_modules/jasmine/bin/jasmine.js",
        "test:sv": "node ./node_modules/jasmine/bin/jasmine.js"
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "^6.1.4",
        "@angular/cdk": "^6.4.6",
        "@angular/common": "^6.1.4",
        "@angular/compiler": "^6.1.4",
        "@angular/core": "^6.1.4",
        "@angular/forms": "^6.1.4",
        "@angular/http": "^6.1.4",
        "@angular/material": "^6.4.6",
        "@angular/platform-browser": "^6.1.4",
        "@angular/platform-browser-dynamic": "^6.1.4",
        "@angular/router": "^6.1.4",
        "angular-in-memory-web-api": "^0.6.1",
        "body-parser": "^1.18.3",
        "compression": "^1.7.1",
        "core-js": "^2.5.4",
        "dotenv": "^4.0.0",
        "errorhandler": "^1.5.0",
        "express": "^4.16.3",
        "express-flash": "0.0.2",
        "express-session": "^1.15.6",
        "express-validator": "^5.3.0",
        "js-yaml": "^3.12.0",
        "log4js": "^3.0.4",
        "lusca": "^1.5.2",
        "passport": "^0.4.0",
        "passport-local": "^1.0.0",
        "pug": "^2.0.3",
        "rxjs": "^6.2.2",
        "zone.js": "^0.8.26"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "~0.7.4",
        "@angular/cli": "~6.1.3",
        "@angular/compiler-cli": "^6.1.4",
        "@angular/language-service": "^6.1.4",
        "@types/body-parser": "^1.16.8",
        "@types/compression": "^0.0.35",
        "@types/dotenv": "^4.0.2",
        "@types/errorhandler": "^0.0.32",
        "@types/express": "^4.11.1",
        "@types/express-flash": "0.0.0",
        "@types/express-session": "^1.15.8",
        "@types/express-validator": "^3.0.0",
        "@types/jasmine": "~2.8.6",
        "@types/jasminewd2": "~2.0.3",
        "@types/js-yaml": "^3.11.2",
        "@types/lusca": "^1.5.0",
        "@types/node": "^10.5.2",
        "@types/passport": "^0.4.3",
        "@types/passport-local": "^1.0.32",
        "@types/shelljs": "^0.7.8",
        "codelyzer": "~4.2.1",
        "jasmine-core": "~3.2.0",
        "jasmine-spec-reporter": "~4.2.1",
        "karma": "~3.0.0",
        "karma-chrome-launcher": "~2.2.0",
        "karma-coverage-istanbul-reporter": "~2.0.0",
        "karma-jasmine": "~1.1.1",
        "karma-jasmine-html-reporter": "^1.2.0",
        "nodemon": "^1.18.3",
        "protractor": "~5.3.0",
        "shelljs": "^0.8.1",
        "ts-node": "~7.0.0",
        "tsconfig-paths": "^3.5.0",
        "tslint": "^5.11.0",
        "typescript": "~2.9.2"
    }
}